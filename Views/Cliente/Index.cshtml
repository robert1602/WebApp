@using WebApp.Models;
@model List<ClienteCLS>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table table-bordered table-hover ">
    <thead>
        <tr>
            <td width="10">@Html.LabelFor(p => Model[0].iidcliente)</td>
            <td width="10">@Html.LabelFor(p => Model[0].nombre)</td>
            <td width="10">@Html.LabelFor(p => Model[0].apPaterno)</td>
            <td width="10">@Html.LabelFor(p => Model[0].apMaterno)</td>
            <td width="10">@Html.LabelFor(p => Model[0].telefonoFijo)</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.iidcliente</td>
            <td>@item.nombre</td>
            <td>@item.apPaterno</td>
            <td>@item.apMaterno</td>
            <td>@item.telefonoFijo</td>
        </tr>

        }


    </tbody>
</table>
